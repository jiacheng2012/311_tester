     background-color:qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
         stop: 0 #F0FFFF, stop: 1 #C6E2FF);
}*/

QGroupBox {
        border: 1px solid #6593CF;
        border-radius: 5px;
        margin-top: 15px;
        padding-top:15px;
        font-size:15px;
        font-weight:800;
}

QGroupBox::title {
        subcontrol-origin: margin;
        subcontrol-position: top center;
        padding: 5px 3px;
        color: white;
        background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(0,153,255, 200),
        stop:0.5 rgba(51,153,255,255),stop:1 rgba(0,153,255,200));
        border-radius: 5px;
        border: 1px solid #6593CF;
}

QLabel {
     border-radius: 4px;
     padding: 2px;
     border-style: flat;
}
/*global style of QTabWidget begins*/
QTabWidget::pane {
        border-top: 2px solid dodgerblue;
}

QTabWidget::tab-bar {
        left: 10px;
}

QTabBar::tab {
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                             stop: 0 rgba(214,232,255,200),
                             stop: 0.8 rgba(214,232,255,255));

        border-bottom-color: #C2C7CB; /* same as the pane color */
        border-top-left-radius: 20;
        border-top-right-radius: 4px;
        min-width: 20ex;
        padding: 3px 8px;
}

QTabBar::tab:selected, QTabBar::tab:hover {
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                             stop: 0.08 rgba(255,255,255,205),
                             stop: 0.8 rgba(255,232,153,255) );
}

QTabBar::tab:selected {
color:#3399FF;
border-color: #9B9B9B;
border-bottom-color: #C2C7CB;
background: rgba(253,253,254,255);
}

QTabBar::tab:!selected {
   margin-top: 2px;
}
/*global style of QTabWidget ends*/
/*global style of QListView begins*/
QListView {
     show-decoration-selected: 1; /* make the selection span the entire width of the view */
 }
 QListView::item:selected {
     border: 0;
 }

 QListView::item:selected:!active {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #ABAFE5, stop: 1 #00CCFF);
 }

 QListView::item:selected:active {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #3399FF, stop: 1 #0272BC);
 }

 QListView::item:hover {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #FAFBFE, stop: 1 #DCDEF1);
 }
/*global style of QListView ends*/
/*global style of QComboBox begins*/
QComboBox::disabled{
    color:wheat;
    background:royalblue;
}
QComboBox:on { /* shift the text when the popup opens */
    padding-top: 3px;
    padding-left: 4px;
}
/*global style of QComboBox ends*/
QSpinBox:focus{background:#CCFF99;}
QTableWidget {
     selection-background-color: rgba(0,153,255,200);
}
CMainWindow{
    background:qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(122,150,223, 255),
    stop:0.35 rgba(216,228,248, 255));
}
CTestSettingWindow{
