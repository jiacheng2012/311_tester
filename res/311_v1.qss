*{
        font-family:"微软雅黑";
}
QPushButton
{
    padding: -1;
    min-width:50px;
    min-height:20px;
    border-radius: 6px;
    border-style:flat;
    /*background:qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 rgba(80,140,255,255),
    stop:0.5 rgba(135,206,250,255),
    stop:1 rgba(80,140,255,255));*/
    color:#3399FF;
    background: rgba(253,253,254,255);
}
QPushButton:disabled
{
    color:gray;
    background:darkgray;
}
QPushButton:focused {
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 rgba(255,240,160,240)
                             stop: 0.5 rgba(255,216,76,255),
                             stop: 1 rgba(255,240,160,240) );
}
QPushButton:hover {
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 rgba(255,240,160,240)
                             stop: 0.5 rgba(255,216,76,255),
                             stop: 1 rgba(255,240,160,240) );
}
QPushButton:pressed{
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 rgba(255,216,76,240)
                             stop: 0.5 rgba(255,200,50,255),
                             stop: 1 rgba(255,216,76,240) );
}

